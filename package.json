{
  "name": "BoringIO",
  "version": "1.0.9",
  "description": "First official release with AutoUpdater",
  "main": "login/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager ./ --overwrite --platform=all --arch=x64 --out ./builds --overwrite --icon=./icon.ico",
    "deploy": "electron-builder --publish always"
  },
  "build": {
    "appId": "org.electron.Pesa2000",
    "directories": {
      "output": "builds"
    },
    "publish": [
      {
        "provider": "github",
        "private": false,
        "owner": "pesa2000",
        "repo": "BoringIOUpdates"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pesa2000/BoringIOUpdates.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "crypto": "^1.0.1",
    "currencies-exchange-rates": "^1.0.2",
    "echarts": "^4.9.0",
    "electron-differential-updater": "^4.3.2",
    "electron-is-packaged": "^1.0.2",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5",
    "firefox-profile": "^2.0.0",
    "got": "^11.7.0",
    "internet-available": "^1.0.0",
    "jquery": "^3.5.1",
    "jsdom": "^16.4.0",
    "moment": "^2.27.0",
    "money": "^0.2.0",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "os-utils": "0.0.14",
    "r2": "^2.0.1",
    "selenium": "^2.20.0",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "stockx-api": "^1.0.9",
    "stripe": "^8.98.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.6.2",
    "electron-packager": "^15.1.0",
    "electron": "^9.2.0",
    "electron-builder": "^22.8.1"
  }
}
